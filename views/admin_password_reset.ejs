                        $('#blue-overlay .submit.button').html('Contact the property on your shortlist');
                    }
                    $content.show();
                });
            }
        });

        /***************************/
        /** Shortlist Form Submit **/
        /***************************/

        $(document).on('submit', '#frmSendMessage', function(event){
            //console.log('sumbitted form');
            event.preventDefault();
            $('.message.result').hide();
            $('.message.result').children().remove();
            $('#loaderImage').css('display', 'inline-block');
            $('button.submit').attr('disabled', 'disabled');
            $('li.error').removeClass('error');
            $('li .message').not('.result, .validation').remove();
            var $form = $(this);

            $.ajax({
                type: "POST",
                url: ajaxurl,
                dataType: 'json',
                data: {
                    'action': 'submit_qcl_form',
                    'form_data': $(this).serialize(),
                    '_ajax_nonce' : sessiondata.nonce
                },
                'error': function() {
                    $('#loaderImage').hide();
                    $('button.submit').removeAttr('disabled');
                },
                'success': function(data, textStatus, jqXHR) {
                    $('#loaderImage').hide();
                    $('button.submit').removeAttr('disabled');
                    if (data.status == 0) {
                        $('.message.validation').children().remove();
                        $('.message.validation').append('<p>' + data.message + '</p>');
                        $('.message.validation').show();

                        for (var i=0; i<data.errors.length; i++) {
                            var $group = $('.' + data.errors[i].name + '-group', $form);
                            $group.addClass('error');
                            $('<span class="message">' + data.errors[i].message + '</span>').appendTo($group);
                        }
                        $('html, body').scrollTop($('#contact-area').position().top);
                        $('.message.validation').fadeIn();
                    } else {
                        $('.message.validation').fadeOut();
                        
                        var oldDocWrite = document.write; // save old doc write

                        document.write = function(node){ // change doc write to be friendlier, temporary
                            $("body").append(node);
                        };

                        $.getScript("../www.googleadservices.com/pagead/f.txt", function() {

                            setTimeout(function() { // let the above script run, then replace doc.write
                                document.write = oldDocWrite;
                            }, 100);

                        });

                        trackConversionEvent();

                        $('#blue-overlay form').hide();
                        $('#blue-overlay form').get(0).reset();
                        $('#blue-overlay h1').html('Thank You!')
                        $('#blue-overlay h1+p').html(data.message);
                    }
                }
            });
        });

        (function() {
            var _fbq = window._fbq || (window._fbq = []);
            if (!_fbq.loaded) {
                var fbds = document.createElement('script');
                fbds.async = true;
                fbds.src = '../connect.facebook.net/en_US/fbds.js';
