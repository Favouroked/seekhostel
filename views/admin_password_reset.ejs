    require(['jquery', 'session', 'shortlist2'], function($, sessiondata){

        $('.contact-shortlist').on('shortlist:contact', function(event, session){
            console.log(session);
            if (isMobile.phone) {
                $('#wrapper').addClass('form');
                $.when(openFullOverlay()).then(function(){
                    var $content = $('#shortlist-contact').clone(true);
                    $('#blue-overlay .content').append($content);
                    if (session.length > 1) {
                        $('#blue-overlay .submit.button').html('Contact the ' + session.length + ' properties on your shortlist');
