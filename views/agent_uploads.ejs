                    </div>


                    <a class="featured-flag sprite"
                       href="/hostel/<%= hostels[i]._id %>"">Featured</a>


                    <div class="listing-detail">
                        <h3><strong><%= hostels[i].location %> &bull;</strong> Fresh Student Living</h3>
                        <h2><a href="/hostel/<%= hostels[i]._id %>"><%= hostels[i].location %></a>
                        </h2>
                        <div class="listing-price-holder">
                            <a href="/hostel/<%= hostels[i]._id %>">
                                <span>Rooms from</span>
                                <span class="listing-price"><span><em>#<%= hostels[i].price%><span class="pence">.00</span></em> Per year</span></span>
                            </a>
                        </div>
                    </div>
                </article>
                <% } %>
            </div>

        </section>

    </div>

