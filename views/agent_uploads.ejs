<div id="wrapper">


    <header id="page-header" class="">


        <div id="banner" style="background: purple; no-repeat center center / cover;"></div>


        <script>
            var setBannerHeight;
            (setBannerHeight = function () {
                if (verge.viewportH() > 700) {
                    var vh = verge.viewportH();
                    var bh = vh - 200;
                    //    console.log(bh);
                    if (bh < 990) {
                        document.getElementById('banner').style.height = bh + 'px';
                        document.getElementById('banner').style.backgroundImage = '#)';
                    } else {
                        document.getElementById('banner').style.backgroundImage = '#)';
                    }
                } else {
                    document.getElementById('banner').style.height = '482px';
                    document.getElementById('banner').style.backgroundImage = '#)';
                }
            });
            window.onresize = function (event) {
                setBannerHeight();
            };
        </script>
        <a id="logo" href="/">
            <img src="/wp-content/themes/msh/images/logo.jpg" alt="SeekHostel.com"/>
