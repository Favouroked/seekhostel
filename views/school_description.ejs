<script>
    require(['jquery'], function ($) {

        $(document).ready(function () {

            $(document).on('submit', '#newsletter-signup form, #blue-overlay #newsletter-form form', function (event) {
                event.preventDefault();

                var $form = $(this);

                $('.message.result', $form).hide();
                $('.message.result', $form).children().remove();

                $.ajax({
                    type: "POST",
                    url: mcurl,
                    dataType: 'json',
                    data: {
                        'email': $('.required.email', $form).val()
                    },
                    'error': function () {
                    },
                    'success': function (data, textStatus, jqXHR) {
                        if (data.status == 'error') {
                            if (data.code == 214) {
                                var message = 'Ah..Looks like you are already on our list.';
                            } else {
                                switch (data.error) {
                                    case 'The email parameter should include an email, euid, or leid key' :
                                        var message = 'Ah…we need your email first!';
                                        break;
                                    case 'An email address must contain a single @' :
                                        var message = 'Almost there! But your email needs to have a single "@"';
                                        break;
                                    case 'The domain portion of the email address is invalid (the portion after the @: )' :
                                        var message = 'Hmm, please check the second part of your email (after the @)';
                                        break;
                                    case 'The username portion of the email address is invalid (the portion before the @: )' :
                                        var message = 'Oh, please check the first part of your email address (before the @)';
                                        break;
                                    default:
                                        var message = data.error;
                                        break;
                                }
                            }
                            $('.message.result', $form).children().remove();
                            $('.message.result', $form).html('<p>' + message + '</p>');
                            $('.message.result', $form).fadeIn();
                        } else {
                            $('.message.result', $form).html('<p>Awesome! You are now signed up to our newsletter</p>');
                            $('.message.result', $form).fadeIn();
                            $form[0].reset();
                        }
                    }
                });
            });
        });

    });
</script>

